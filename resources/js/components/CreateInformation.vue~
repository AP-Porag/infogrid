            crossover_minimum_grade:{
                required: requiredIf(function () {
                    return this.showItemTypeCrossoverBox // return true if this field is required
                }),
            },
            crossover_item_type:{
                required: requiredIf(function () {
                    return this.showItemTypeCrossoverBox // return true if this field is required
                }),
            },
        }
    }

}
